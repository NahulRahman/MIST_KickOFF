<AnchorPane prefHeight="800.0" prefWidth="1200.0" style="-fx-background-color: #0073CE;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.nahulthejoker.HelloController">
